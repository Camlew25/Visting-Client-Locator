    <div id="map"></div>
    <script>
      var map;
      function initMap() {
  var map = new google.maps.Map(document.getElementById('map'), {
    center: {lat: 41.850033, lng: -87.6500523},
    zoom: 5
  });

  var d1Layer = new google.maps.FusionTablesLayer({
    query: {
      select: '\'Full address\'',
     from: '1NhxMd4P-KLtnNMEugJpqj3eLcAHNBijUdiyLi6za'

    }
  });
  d1Layer.setMap(map);
  
  var d2Layer = new google.maps.FusionTablesLayer({
    query: {
      select: '\'Full address\'',
      from: '1Bqd8XaOhJHs3GZrIwJn-FWQiLgUI5mPGCGi-0oH0'
    }
  });
  d2Layer.setMap(map);

  var d3Layer = new google.maps.FusionTablesLayer({
    query: {
      select: '\'Full address\'',
      from: '1QcXS_64Rmv_wDCnY33e4-fVbdpX1HkY44LEDoKGh'
    }
  });
  d3Layer.setMap(map);



}
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDmtDTGroz9uJrGANGf9HV8kbGtfx-at_k&callback=initMap"
    async defer></script>
  </body>
